{
  "name": "race-tracker-pro",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "node src/test/e2e/e2e-test-runner.js",
    "test:e2e:ci": "node src/test/e2e/e2e-test-runner.js --headless",
    "test:e2e:complete": "node run-e2e-test.js",
    "test:seed": "node -e \"import('./src/test/e2e/test-data-seeder.js').then(m => m.seedCompleteTestDataset())\"",
    "test:seed:minimal": "node -e \"import('./src/test/e2e/test-data-seeder.js').then(m => m.seedMinimalTestDataset())\"",
    "test:clear": "node -e \"import('./src/test/e2e/test-data-seeder.js').then(m => m.clearAllTestData())\"",
    "screenshots:base-station": "node capture-base-station-screenshots.js",
    "screenshots:all": "node final-screenshots.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dexie": "^3.2.4",
    "puppeteer": "^24.12.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^4.1.12",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.6",
    "autoprefixer": "^10.4.14",
    "fake-indexeddb": "^6.2.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.27",
    "prop-types": "^15.8.1",
    "react-router-dom": "^7.9.5",
    "react-test-renderer": "^19.2.0",
    "tailwindcss": "^3.3.3",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^3.2.4",
    "vitest-dom": "^0.1.1"
  }
}
